{
  "name": "arcade-app",
  "version": "0.1.0",
  "description": "Distributed Arcade Game App",
  "author": "Olivier Lange",
  "contributors": [
    "Rudi Farkas"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "npm dedupe",
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:rollup": "rollup -c rollup.config.js",
    "watch": "run-p watch:*",
    "watch:rollup": "rollup -w -c rollup.config.js",
    "watch:start": "es-dev-server --root-dir dist --app-index index.html --open --compatibility none"
  },
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "graphql": "^15.3.0",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "es-dev-server": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "rollup": "^2.3.4"
  },
  "engines": {
    "node": "14.x"
  }
}